    <Compile Include="..\Library\Model\Module\Helper\RewardCodeHelper.cs" Link="Helper\RewardCodeHelper.cs" />
    <Compile Include="..\Library\Model\Share\Utility\GameUtility.cs">
      <Link>Share\Utility\GameUtility.cs</Link>
    </Compile>
    <Compile Include="..\Library\Model\Share\Utility\LanguageUtility.cs" Link="Share\Utility\LanguageUtility.cs" />
    <Compile Include="..\Library\Model\Share\Utility\ManagerUtility.cs" Link="Share\Utility\ManagerUtility.cs" />
    <Compile Include="..\Library\Model\Share\Utility\RecordUtility.cs" Link="Share\Utility\RecordUtility.cs" />
	<Compile Include="..\Library\Model\Share\Utility\RoomUtility.cs">
	  <Link>Share\Utility\RoomUtility.cs</Link>
	</Compile>
    <EmbeddedResource Remove="Other\**" />
    <None Remove="Other\**" />
